    "framer-motion": "^5.5.5",
    "fuse.js": "^6.5.3",
    "moment": "^2.29.1",
    "next": "12.0.7",
    "next-contentlayer": "^0.0.33",
    "qs": "^6.10.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "reading-time": "^1.4.0",
